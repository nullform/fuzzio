{
    "name": "nullform/fuzzio",
    "description": "Fuzzy search using similar_text() and levenshtein() functions. Easy to use and safe for multibyte encodings (UTF-8).",
    "type": "library",
    "keywords": ["fuzzy", "search", "similar_text", "levenshtein", "levenshtein distance", "similarity"],
    "homepage": "https://github.com/nullform/fuzzio",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Novikov"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "php": "^5.6",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Nullform\\Fuzzio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nullform\\Fuzzio\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
